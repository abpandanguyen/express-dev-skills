<%- include('../partials/header') %>

<h1>Edit Skill #<%= skill.id %></h1>
<form action="/skills/<%= skill.id %>?_method=PUT" method="POST" autocomplete="off">
  <input type="text" name="skill" value="<%= skill.skill %>">t
  <br>
  <input type="checkbox" name="proven" id="proven" <%= skill.proven ? 'checked' : '' %>>
  <label for="proven">Proven?</label>
  <br>
  <input type="submit" value="SAVE SKILL">
</form>


